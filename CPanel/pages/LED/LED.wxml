    <scroll-view id="scroll-view" scroll-into-view="{{Chandelier_itemview}}" scroll-x class="scroll-x" scroll-left="{{Chandelier_scrollLeft}}" scroll-with-animation="true" bindscroll="scrollMove" style="position: relative;margin-top: 40px; left: 30px;width: 404rpx; height: 158rpx; display: flex; box-sizing: border-box;">
        <view wx:for="{{Chandelier_datalist}}" wx:key="index" class="view-parent">
            <view id="{{'item'+index}}" bindtap="Chandelier_Checked" class="iconfont .view-Missing{{Chandelier_selectItemIndex == index?'On':''}}" data-index="{{index}}"> </view>
        </view>
    </scroll-view>
