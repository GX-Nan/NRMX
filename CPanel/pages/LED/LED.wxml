        <view id="Up" bindtap="Chandelier_Status" class="iconfont .icon-xiangshang " style="position: relative;margin-top: 15px; width: 100rpx; background-color: {{ChandelierBackgroundColor_Up}};  color:{{ChandelierColor_Font_Up}};height: 100rpx; display: block; margin-left: -50px;box-sizing: border-box; left: -90rpx;  background-size: 100% 100%;font-size: 50px;box-shadow: 0px 1px 12px -6px black;border-radius: 8px;"></view>
        <view id="Stop" bindtap="Chandelier_Status" class="iconfont .icon-24gf-stopSquare" style="position: relative; margin-top: -50px;width: 100rpx; background-color: {{ChandelierBackgroundColor_Stop}};  color:{{ChandelierColor_Font_Stop}}; height: 100rpx; display: block; box-sizing: border-box; margin-left: -0rpx;  background-size: 100% 100%;font-size: 50px;box-shadow: 0px 1px 12px -6px black;border-radius: 8px;"></view>
